# Production Environment Configuration
# This file should be created from .env file with production URL
# The production domain will be read from REACT_APP_PRODUCTION_DOMAIN
REACT_APP_PRODUCTION_DOMAIN=your-production-domain.com
REACT_APP_API_URL=https://your-production-domain.com/api/v1
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_PRIMARY_COLOR=#2563eb
REACT_APP_THEME=light
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_SUPPORTED_LANGUAGES=en,fa
NODE_ENV=production